<!doctype html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="description" content="Live stats web page"/>
    <meta name="author" content="James Whybrow @ jw.network"/>
    <title>Live Stats</title>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.5/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="//cdn.rawgit.com/tonystar/bootstrap-plugins/master/dist/css/bootstrap-plugins.min.css"/>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css"/>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/github-fork-ribbon-css/0.1.1/gh-fork-ribbon.min.css"/>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>   
</head>
<body>
    <div class="container">
        <h1>Live Stats</h1>
        <div class="row">
            <% vars.forEach(function(intvars) { %>
                    <div class="well" style="margin: 5px;">
                        <h3>Name: <%= intvars[0] %></h3>
                        <h3 id="<%= intvars[0] %>cpu">CPU: <%= intvars[1] %></h3>
                        <h3 id="<%= intvars[0] %>ram">RAM: <%= intvars[2] %></h3>
                        <h3>IP: <%= intvars[3] %></h3>
                    </div>
            <% }); %>
            <% if (vars == "") { %>
                <h3>No devices online.</h3>
            <% } %>
        </div>
    </div>
</body>
</html>